{"code":"import _escape from 'lodash.escape';\r\nimport _unescape from 'lodash.unescape';\r\nimport _escapeRegExp from 'lodash.escaperegexp';\r\n/**\r\n *\r\n * @param str\r\n */\r\nexport function escape(str) {\r\n    return _escape(str);\r\n}\r\n/**\r\n *\r\n * @param str\r\n */\r\nexport function unescape(str) {\r\n    return _unescape(str);\r\n}\r\n/**\r\n *\r\n * @param str\r\n * @returns {any}\r\n */\r\nexport function escapeRegExp(str) {\r\n    return _escapeRegExp(str);\r\n}\r\n/**\r\n *\r\n * @param str\r\n * @returns {any}\r\n */\r\nexport function capitalize(str) {\r\n    return str.charAt(0).toUpperCase() + str.slice(1).toLowerCase();\r\n}\r\n/**\r\n *\r\n * @param str\r\n * @returns {string}\r\n */\r\nexport function camelCase(str) {\r\n    var camelCase = function (str) {\r\n        var string = str.toLowerCase().replace(/[^A-Za-z0-9]/g, ' ').split(' ')\r\n            .reduce(function (result, word) { return result + capitalize(word.toLowerCase()); });\r\n        return string.charAt(0).toLowerCase() + string.slice(1);\r\n    };\r\n    var capitalize = function (str) { return str.charAt(0).toUpperCase() + str.toLowerCase().slice(1); };\r\n    return camelCase(str);\r\n}\r\n/**\r\n *\r\n * @param str\r\n * @returns {string}\r\n */\r\nexport function kebabCase(str) {\r\n    return str.replace(/([a-z])([A-Z])/g, '$1-$2').replace(/\\s+/g, '-').toLowerCase();\r\n}\r\n//# sourceMappingURL=index.js.map","map":{"version":3,"file":"index.js","sourceRoot":"","sources":["../src/string/index.ts"],"names":[],"mappings":"AAAA,OAAO,OAAO,MAAM,eAAe,CAAC;AACpC,OAAO,SAAS,MAAM,iBAAiB,CAAC;AACxC,OAAO,aAAa,MAAM,qBAAqB,CAAC;AAEhD;;;GAGG;AACH,MAAM,iBAAiB,GAAG;IACxB,MAAM,CAAC,OAAO,CAAC,GAAG,CAAC,CAAC;AACtB,CAAC;AAED;;;GAGG;AACH,MAAM,mBAAmB,GAAG;IAC1B,MAAM,CAAC,SAAS,CAAC,GAAG,CAAC,CAAC;AACxB,CAAC;AAED;;;;GAIG;AACH,MAAM,uBAAuB,GAAG;IAC9B,MAAM,CAAC,aAAa,CAAC,GAAG,CAAC,CAAC;AAC5B,CAAC;AAED;;;;GAIG;AACH,MAAM,qBAAqB,GAAG;IAC5B,MAAM,CAAC,GAAG,CAAC,MAAM,CAAC,CAAC,CAAC,CAAC,WAAW,EAAE,G